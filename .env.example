# Конфигурация для продакшена
# Скопируйте этот файл в .env и измените значения

# JWT секретный ключ (используйте случайную строку)
SECRET_KEY=your-secret-key-change-in-production-123456789

# Алгоритм шифрования JWT
ALGORITHM=HS256

# Время жизни токена в минутах
ACCESS_TOKEN_EXPIRE_MINUTES=30

# База данных (для будущего использования)
# DATABASE_URL=postgresql://user:password@localhost/credit_scoring
